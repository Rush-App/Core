{
    "name": "rush-app/core",
    "type": "library",
    "description": "Library for simplifying development on Laravel.",
    "authors": [
        {
            "name": "RushApp team",
            "email": "info.rushapp@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.2",
        "laravel/framework": "^9|^10",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1",
        "mockery/mockery": "^1",
        "phpunit/phpunit": "^9|^10"
    },
    "autoload": {
        "psr-4": {
            "RushApp\\Core\\": "src/",
            "RushApp\\Core\\Database\\Factories\\": "database/factories"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RushApp\\Core\\CoreServiceProvider"
            ]
        }
    }
}
